create table main (
id text not null,
title text not null,
_cast text array[3],
photos text array[3],
storyline text,
plotKeyWords text array[4],
taglines varchar(90),
genres text array[3],
movieRating varchar(90),
mpaa varchar(20),
officialSites text array[1],
country varchar(90),
languages varchar(90),
releaseDate varchar(90),
aKa varchar(20),
filmingLocations text array[3],
budget varchar(90),
openingWeekend varchar(90),
gross varchar(90),
cumulative varchar(90),
productionCo varchar(90),
runtime varchar(90),
soundMix varchar(20),
color varchar(20),
aspectRation varchar(20),
trivia varchar(20),
goofs varchar(20),
quotes varchar(20),
crazyCredits varchar(20),
connections varchar(20),
soundtracks text array[3],
faq text array[6],
primary key(id)
);
